<p id="notice"><%= notice %></p>

<h1>Orders</h1>

<table>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.name %></td>
        <td><%= link_to 'Get shipping label', get_shipping_label_order_path(order.id) %></td>
        <td><%= link_to 'Get shipping + return label', get_shipping_with_return_label_order_path(order.id) %></td>
        <td><%= link_to 'Show', order %></td>
        <td><%= link_to 'Edit', edit_order_path(order) %></td>
        <td><%= link_to 'Destroy', order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order', new_order_path %>
